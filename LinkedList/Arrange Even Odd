class ListNode{
    int val;
    ListNode next;
    ListNode(int val,ListNode next){
        this.val=val;
        this.next=next;
    }
}
public class Random {
    static ListNode oddEvenList(ListNode head){
        if (head==null)return null;
        ListNode odd=head;
        ListNode even=head.next;
        ListNode evenHead=even;
        while (even!=null&&even.next!=null){
            odd.next=even.next;
            odd=odd.next;
            even.next=odd.next;
            even=even.next;
        }
        odd.next=evenHead;
        return head;
    }
